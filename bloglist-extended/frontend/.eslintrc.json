{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true,
        "cypress/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest",
        "cypress"
    ],
    "rules": {
        "react/prop-types": 0,
        "react/function-component-definition": 0,
        "react/jsx-one-expression-per-line": 0,
        "no-restricted-globals": 0,
        "prefer-arrow-callback": 0
    }
}
